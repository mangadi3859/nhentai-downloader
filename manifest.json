{
    "manifest_version": 3,
    "name": "Nhentai Downloader",
    "author": "https://github.com/mangadi3859",
    "description": "Download any nhentai gallery as PDF file",
    "version": "1.1.0",
    "icons": {
        "128": "/assets/logo.png"
    },
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["https://*.nhentai.net/g/*/"],
            "js": ["/dist/js/content.js"],
            "run_at": "document_idle"
        }
    ]
}
